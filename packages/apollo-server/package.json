{
  "name": "apollo-server",
  "version": "2.0.0-rc.6",
  "description": "Production ready GraphQL Server",
  "author": "opensource@apollographql.com",
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc",
    "prepublish": "npm run compile",
    "watch": "tsc -w",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apollographql/apollo-server/tree/master/packages/apollo-server"
  },
  "keywords": [
    "GraphQL",
    "Apollo",
    "Server",
    "Javascript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apollographql/apollo-server/issues"
  },
  "homepage": "https://github.com/apollographql/apollo-server#readme",
  "dependencies": {
    "apollo-server-core": "^2.0.0-rc.6",
    "apollo-server-express": "^2.0.0-rc.6",
    "express": "^4.0.0",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "^3.0.4"
  },
  "devDependencies": {
    "@types/request": "^2.47.0",
    "request": "^2.87.0",
    "@types/jest": "^23.1.2",
    "jest": "^23.2.0",
    "ts-jest": "^22.4.6"
  },
  "peerDependencies": {
    "graphql": "^0.12.0 || ^0.13.0 || ^14.0.0"
  },
  "typings": "dist/index.d.ts",
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(ts|js)$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "testRegex": "apollo-server/src.*/__tests__/.*$",
    "roots": [
      "../../"
    ],
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    }
  }
}
